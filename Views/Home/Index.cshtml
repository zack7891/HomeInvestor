@model HomeInvestor.Models.HouseModel


<!-- Banner -->
<section id="banner">
    <div class="inner">
        <h2>Please Enter Information for Smart Contract</h2>
    </div>
</section>

<section>

    @using (Html.BeginForm("AddContract", "Home", FormMethod.Post))
    {
        @Html.LabelFor(m => m.EthAddress)
        @Html.TextBoxFor(model => model.EthAddress)

        @Html.LabelFor(m => m.Address)
        @Html.TextBoxFor(model => model.Address)

        @Html.LabelFor(m => m.City)
        @Html.TextBoxFor(model => model.City)

        @Html.LabelFor(m => m.State)
        @Html.TextBoxFor(model => model.State)

        @Html.LabelFor(m => m.Zipcode)
        @Html.TextBoxFor(model => model.Zipcode)

        @Html.LabelFor(m => m.Bedrooms)
        @Html.TextBoxFor(model => model.Bedrooms)

        @Html.LabelFor(m => m.Bathrooms)
        @Html.TextBoxFor(model => model.Bathrooms)
        
        @Html.LabelFor(m => m.HomeValue)
        @Html.TextBoxFor(model => model.HomeValue)
        <input type="file" name="ImageFile" />

        <br />
        <br />
        <input type="submit" value="Submit" class="btn btn-primary" />
    }


</section>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
